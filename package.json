{
  "name": "electron-react",
  "version": "1.0.0",
  "description": "",
  "main": "app/main/index.js",
  "scripts": {
    "build":"cd app/renderer/src/main  && npm run build",
    "pack:win": "npm run build && electron-builder --win32 --ia32",
    "rebuild": "npm rebuild --runtime=electron --target=7.1.8 --disturl=https://atom.io/download/atom-shell --abi=51",
    "commit": "npx git-cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run start:renderer\"  \"wait-on http://10.10.2.68:3000 && npm run start:main\"",
    "start:main": "electron .",
    "start:renderer": "cd app/renderer/src/main && npm start "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "robotjs": "^0.6.0",
    "vkey": "^1.0.1",
    "wait-on": "^6.0.0",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron": "^7.1.8",
    "electron-builder": "^22.3.2",
    "electron-builder-squirrel-windows": "^22.13.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2"
  },
  "homepage": "./",
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitizen": {
    "path": "./node_modules/cz-customizable"
  },
  "build":{
    "appid":"zsl",
    "productName":"zsl1.1",
    "files":"app",
    "asar": false,
    "extraFiles":[
      "app/renderer/**/src"
    ],
    "directories":{
      "buildResources":"resource",
      "output":"relase"
    }
  }
}
