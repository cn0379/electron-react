{
  "name": "electron-react",
  "version": "1.0.0",
  "description": "",
  "main": "app/main/index.js",
  "scripts": {
    "rebuild": "npm rebuild --runtime=electron --target=13.2.1 --disturl=https://atom.io/download/atom-shell --abi=83",
    "commit": "npx git-cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run start:renderer\"  \"wait-on http://10.10.2.68:3000 && npm run start:main\"",
    "start:main": "electron .",
    "start:renderer": "cd app/renderer/src/main && npm start "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "robotjs": "^0.6.0",
    "vkey": "^1.0.1",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "commitizen": "^4.2.4",
    "electron": "^13.2.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2"
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitizen": {
    "path": "./node_modules/cz-customizable"
  }
}
